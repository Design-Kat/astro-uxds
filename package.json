{
	"name": "astro-website",
	"version": "0.0.1",
	"description": "Generates the public facing Astro UX Guidelines.",
	"main": "index.js",
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"build": "rm -rf _content && git clone https://bitbucket.org/rocketcom/astro-guidelines.git _content  && eleventy",
		"serve": "rm -rf _content && git clone https://bitbucket.org/rocketcom/astro-guidelines.git _content  && eleventy --serve",
		"build:errata": "rm -rf _content && git clone https://bitbucket.org/rocketcom/astro-guidelines.git -b errata _content  && eleventy",
		"serve:errata": "rm -rf _content && git clone https://bitbucket.org/rocketcom/astro-guidelines.git -b errata _content  && eleventy --serve",
		"build:draft": "rm -rf _content && git clone https://bitbucket.org/rocketcom/astro-guidelines.git -b draft _content  && eleventy",
		"serve:draft": "rm -rf _content && git clone https://bitbucket.org/rocketcom/astro-guidelines.git -b draft _content  && eleventy --serve"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"@11ty/eleventy": "^0.9.0",
		"clean-css": "^4.2.1",
		"gulp": "^4.0.2",
		"gulp-imagemin": "^6.1.0",
		"markdown-it-anchor": "^5.2.5",
		"markdown-it-container": "^2.0.0",
		"markdown-it-div": "^1.1.0",
		"markdown-it-implicit-figures": "^0.9.0"
	},
	"dependencies": {}
}
